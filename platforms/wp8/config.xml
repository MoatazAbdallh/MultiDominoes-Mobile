<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1900106" id="com.espritsolutions.multidominoes" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Toast">
        <param name="wp-package" value="Toast" />
    </feature>
    <feature name="Notification">
        <param name="wp-package" value="Notification" />
    </feature>
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <name>MultiDominoes</name>
    <description>
         Play Dominoes with your friends through your Smart TV
    </description>
    <author email="info@esprit-solutions.com" href="http://esprit-solutions.com">
        eSprit Solutions Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
